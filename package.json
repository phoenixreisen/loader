    "mithril": "^2.0.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-file-loader": "^1.1.1",
    "babel-plugin-inline-svg": "^1.0.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "del-cli": "^3.0.0",
    "jsdom": "^15.1.1",
    "mithril-node-render": "^2.3.2",
    "mithril-query": "^3.0.1",
