{
  "name": "@phoenixreisen/loader",
  "version": "0.0.2",
  "description": "Ladeanzeige(n) für API-Abfragen oder ähnliches.",
  "homepage": "https://design-system.phoenixreisen.net",
  "main": "dist/loader.view.js",
  "scripts": {
    "compile:js": "babel src --out-dir dist",
